{
  "name": "bt-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint --fix",
    "start": "nodemon --watch vue.config.js --watch package-lock.json --exec npm run dev",
    "dev": "npm-run-all --parallel --silent backend-api frontend",
    "ui": "vue ui",
    "backend-api": "json-server src/agGridData.json --port 8081",
    "frontend": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "fetch-rowData": "node src/fetch-rowData.js",
    "severity": "node ./severity.js"
  },
  "dependencies": {
    "ag-grid-community": "^19.1.1",
    "ag-grid-enterprise": "^19.1.1",
    "ag-grid-vue": "^19.1.1",
    "register-service-worker": "^1.5.2",
    "vue": "^2.5.17",
    "vue-global-events": "^1.0.3",
    "vue-mdc-adapter": "^0.18.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.4",
    "@vue/cli-plugin-pwa": "^3.1.1",
    "@vue/cli-service": "^3.1.3",
    "clean-terminal-webpack-plugin": "^1.1.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-vue": "git+https://github.com/vuejs/eslint-plugin-vue.git",
    "json-server": "^0.14.0",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.3",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.17",
    "webpack-build-notifier": "^0.1.30"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
