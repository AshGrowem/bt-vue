{
  "name": "bt-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "backend": "json-server src/agGridData.json --port 8081",
    "dev": "npm-run-all --parallel --silent backend frontend",
