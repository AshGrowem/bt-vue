{
  "name": "bt-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "backend": "json-server src/agGridData.json --port 8081",
    "build": "vue-cli-service build --watch",
    "dev": "npm-run-all --parallel --silent backend frontend",
    "fetch-rowData": "node src/fetch-rowData.js",
    "frontend": "vue-cli-service serve",
    "frontend-prod": "vue-cli-service serve --mode production",
    "prod": "npm-run-all --parallel --silent backend frontend-prod",
    "start": "nodemon --quiet --watch vue.config.js --exec npm run dev",
    "ui": "vue ui"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "ag-grid-community": "^20.2.0",
    "ag-grid-enterprise": "^20.2.0",
    "ag-grid-vue": "^20.2.0",
    "core-js": "^2.6.5",
    "deepmerge": "^3.2.0",
    "fibers": "^3.1.1",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-global-events": "^1.1.2",
    "vue-mdc-adapter": "^0.18.2",
    "vue-router": "^3.0.6",
    "vuetify": "^2.0.0-alpha.16",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-plugin-pwa": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "babel-eslint": "^10.0.1",
    "clean-terminal-webpack-plugin": "^2.0.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-vue": "^5.2.2",
    "json-server": "^0.14.2",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "sass": "^1.19.0",
    "sass-loader": "^7.1.0",
    "simple-progress-webpack-plugin": "^1.1.2",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-property-decorator": "^8.1.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2",
    "webpack-build-notifier": "^1.0.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  }
}
