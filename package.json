    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-property-decorator": "^7.3.0",
    "vue-template-compiler": "^2.6.6",
    "vuetify-loader": "^1.2.1",
    "webpack-build-notifier": "^0.1.30"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
